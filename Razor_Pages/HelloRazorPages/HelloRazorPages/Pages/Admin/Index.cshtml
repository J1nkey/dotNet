@page

@{
    Layout = null;
    ViewData["Title"] = "Admin Index";
}
<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width;initial-scale=1.0" />
        @* This is an example of razor syntax in single line *@
        <title>@ViewData["Title"]</title>
    </head>
    <body>
        <h1>Welcome to @ViewData["Title"] page</h1>
        <h2>Current time: @DateTime.Now.ToLongDateString()</h2>
        <p></p>
    </body>
</html>
